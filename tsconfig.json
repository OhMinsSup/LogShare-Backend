{
    "compilerOptions": {
      "outDir": "dist",
      "module": "commonjs",
      "target": "es5",
      "lib": ["es6", "es5", "esnext.asynciterable"],
      "inlineSourceMap": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "downlevelIteration": true
    },
    "include": [
      "./src/**/*"
    ],
    "exclude": [
      "node_modules",
      ".vscode"
    ]
  }